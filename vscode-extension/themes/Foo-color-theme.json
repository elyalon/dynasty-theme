{
  "name": "Foo",
  "semanticHighlighting": true, // Figure out if I need this
  "semanticTokenColors": {
    "stringLiteral": "#beeaff",
    "customLiteral": "#7bc4ff",
    "numberLiteral": "#b5cea8",
    "*.mutable": { "fontStyle": "" },
    "parameter": { "italic": true },
    "const": "#ffdcad",
    "enumMember": "#ffdcad",
    "decorator": "#ffdcad",
    "operator": "#cccccc",
    "operator.unsafe": { "underline": true },
    "function.unsafe": { "underline": true },
    "method.unsafe": { "underline": true }
  },
  "colors": {
    "editorBracketHighlight.foreground1": "#f3f3f3bb",
    "editorBracketHighlight.foreground2": "#ffd45fbb",
    "editorBracketHighlight.foreground3": "#82c7ffbb",
    "editorBracketHighlight.foreground4": "#ff8484bb",

    "sideBar.background": "#222222",
    "statusBar.background": "#222222",
    "tab.inactiveBackground": "#222222",

    "tab.activeBorder": "#070707",
    "tab.unfocusedActiveBorder": "#070707",
    "editor.background": "#070707",

    "titleBar.inactiveForeground": "#f3f3f3a6",
    "titleBar.activeForeground": "#f3f3f3bd",
    "titleBar.inactiveBackground": "#070707",
    "titleBar.activeBackground": "#222222",
    "titleBar.border": "#555",

    "panel.background": "#070707",
    "panel.border": "#555",
    "panelTitle.border": "#555",

    "commandCenter.background": "#ffffff1f",
    "commandCenter.activeBackground": "#ffffff2f",
    "commandCenter.border": "#555",
    "commandCenter.activeBorder": "#777",
    "commandCenter.inactiveBorder": "#444",

    "editorGroupHeader.tabsBackground": "#222222",
    "editorGroupHeader.noTabsBackground": "#222222",
    "editorGroupHeader.tabsBorder": "#555",

    "sideBar.border": "#555",
    "tab.border": "#555",
    "statusBar.border": "#555",
    "editorGroup.border": "#555",
    "tab.activeBorderTop": "#dba74c",
    "tab.selectedBorderTop": "#0000",
    "tab.unfocusedActiveBorderTop": "#555",
    "tab.selectedBackground": "#002544",
    "tab.hoverBackground": "#171717",
    "tab.activeForeground": "#f3f3f3",
    "tab.unfocusedInactiveForeground": "#f3f3f3cb",
    "tab.unfocusedActiveForeground": "#f3f3f3",
    "tab.inactiveForeground": "#f3f3f3",
    "input.background": "#070707",

    "activityBarBadge.background": "#2a72c5",
    "activityBarBadge.foreground": "#ffffff",
    "editor.foreground": "#f3f3f3",

    "sideBarSectionHeader.background": "#3b3b3b",
    "sideBarActivityBarTop.border": "#555",
    "sideBarSectionHeader.foreground": "#f3f3f3e0",

    "activityBar.background": "#222222",
    "activityBar.border": "#555",
    "activityBar.foreground": "#f3f3f3",
    "activityBar.inactiveForeground": "#f3f3f3a6",

    // === BASE COLORS ===
    "borderFocus": "#0088d6",
    "input.border": "#555",
    "foreground": "#f3f3f3",
    "disabledForeground": "#f3f3f385",
    "widget.border": "#555",
    "widget.shadow": "#040404",
    "selection.background": "#60b7ff71",
    "descriptionForeground": "#c0c0c0",
    "errorForeground": "#ffbb56",
    "icon.foreground": "#c5c5c5",
    "sash.hoverBorder": "#0088d6"
  },
  "tokenColors": [
    {
      "scope": "variable",
      "settings": { "foreground": "#f3f3f3" }
    },
    {
      "scope": "punctuation",
      "settings": { "foreground": "#f3f3f3bb" }
    },
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded",
        "string meta.image.inline.markdown",
        "variable.legacy.builtin.python"
      ],
      "settings": { "foreground": "#D4D4D4" }
    },
    { "scope": "emphasis", "settings": { "fontStyle": "italic" } },
    { "scope": "strong", "settings": { "fontStyle": "bold" } },
    { "scope": "header", "settings": { "foreground": "#000080" } },
    { "scope": "comment", "settings": { "foreground": "#818181" } },
    { "scope": "constant.language", "settings": { "foreground": "#ff8484" } },
    {
      "scope": [
        "constant.numeric",
        "keyword.operator.plus.exponent",
        "keyword.operator.minus.exponent"
      ],
      "settings": { "foreground": "#cbffbe" }
    },
    { "scope": "constant.regexp", "settings": { "foreground": "#646695" } },
    { "scope": "entity.name.tag", "settings": { "foreground": "#ff8484" } },
    {
      "scope": ["entity.name.tag.css", "entity.name.tag.less"],
      "settings": { "foreground": "#d7ba7d" }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": { "foreground": "#9cdcfe" }
    },
    {
      "scope": [
        "entity.other.attribute-name.class.css",
        "source.css entity.other.attribute-name.class",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.parent-selector.css",
        "entity.other.attribute-name.parent.less",
        "source.css entity.other.attribute-name.pseudo-class",
        "entity.other.attribute-name.pseudo-element.css",
        "source.css.less entity.other.attribute-name.id",
        "entity.other.attribute-name.scss"
      ],
      "settings": { "foreground": "#d7ba7d" }
    },
    { "scope": "invalid", "settings": { "foreground": "#f0f" } },
    { "scope": "markup.underline", "settings": { "fontStyle": "underline" } },
    {
      "scope": "markup.bold",
      "settings": { "fontStyle": "bold", "foreground": "#ff8484" }
    },
    {
      "scope": "markup.heading",
      "settings": { "fontStyle": "bold", "foreground": "#ff8484" }
    },
    { "scope": "markup.italic", "settings": { "fontStyle": "italic" } },
    {
      "scope": "markup.strikethrough",
      "settings": { "fontStyle": "strikethrough" }
    },
    { "scope": "markup.inserted", "settings": { "foreground": "#b5cea8" } },
    { "scope": "markup.deleted", "settings": { "foreground": "#beeaff" } },
    { "scope": "markup.changed", "settings": { "foreground": "#ff8484" } },
    {
      "scope": "punctuation.definition.quote.begin.markdown",
      "settings": { "foreground": "#6A9955" }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": { "foreground": "#6796e6" }
    },
    { "scope": "markup.inline.raw", "settings": { "foreground": "#beeaff" } },
    {
      "name": "brackets of XML/HTML tags",
      "scope": "punctuation.definition.tag",
      "settings": { "foreground": "#808080" }
    },
    {
      "scope": "meta.preprocessor.string",
      "settings": { "foreground": "#beeaff" }
    },
    {
      "scope": "meta.preprocessor.numeric",
      "settings": { "foreground": "#b5cea8" }
    },
    { "scope": "meta.diff.header", "settings": { "foreground": "#ff8484" } },
    { "scope": "storage", "settings": { "foreground": "#ff8484" } },
    { "scope": "storage.type", "settings": { "foreground": "#ff8484" } },
    {
      "scope": ["storage.modifier", "keyword.operator.noexcept"],
      "settings": { "foreground": "#ff8484" }
    },
    {
      "scope": ["string", "meta.embedded.assembly"],
      "settings": { "foreground": "#beeaff" }
    },
    { "scope": "string.tag", "settings": { "foreground": "#beeaff" } },
    { "scope": "string.value", "settings": { "foreground": "#beeaff" } },
    { "scope": "string.regexp", "settings": { "foreground": "#d16969" } },
    {
      "name": "String interpolation",
      "scope": [
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.section.embedded"
      ],
      "settings": { "foreground": "#ff8484" }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": ["meta.template.expression"],
      "settings": { "foreground": "#d4d4d4" }
    },
    {
      "scope": ["keyword.other", "keyword.control"],
      "settings": { "foreground": "#ff8484" }
    },
    { "scope": "keyword.operator", "settings": { "foreground": "#cccccc" } },
    {
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.alignof",
        "keyword.operator.typeid",
        "keyword.operator.alignas",
        "keyword.operator.instanceof",
        "keyword.operator.logical.python",
        "keyword.operator.wordlike"
      ],
      "settings": { "foreground": "#ff8484" }
    },
    { "scope": "keyword.other.unit", "settings": { "foreground": "#b5cea8" } },
    {
      "scope": [
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php"
      ],
      "settings": { "foreground": "#ff8484" }
    },
    {
      "scope": "constant.sha.git-rebase",
      "settings": { "foreground": "#b5cea8" }
    },
    {
      "name": "coloring of the Java import and package identifiers",
      "scope": [
        "storage.modifier.import.java",
        "variable.language.wildcard.java",
        "storage.modifier.package.java"
      ],
      "settings": { "foreground": "#d4d4d4" }
    },
    {
      "name": "this.self",
      "scope": "variable.language",
      "settings": { "foreground": "#ff8484" }
    },

    // === DARK PLUS ===

    {
      "name": "Function declarations",
      "scope": [
        "entity.name.function",
        "support.function",
        "support.constant.handlebars",
        "source.powershell variable.other.member",
        "entity.name.operator.custom-literal"
      ],
      "settings": { "foreground": "#82c7ff" }
    },
    {
      "name": "Types declaration and references",
      "scope": [
        "support.class",
        "support.type",
        "entity.name.type",
        "entity.name.namespace",
        "entity.other.attribute",
        "entity.name.scope-resolution",
        "entity.name.class",
        "storage.type.numeric.go",
        "storage.type.byte.go",
        "storage.type.boolean.go",
        "storage.type.string.go",
        "storage.type.uintptr.go",
        "storage.type.error.go",
        "storage.type.rune.go",
        "storage.type.cs",
        "storage.type.generic.cs",
        "storage.type.modifier.cs",
        "storage.type.variable.cs",
        "storage.type.annotation.java",
        "storage.type.generic.java",
        "storage.type.java",
        "storage.type.object.array.java",
        "storage.type.primitive.array.java",
        "storage.type.primitive.java",
        "storage.type.token.java",
        "storage.type.groovy",
        "storage.type.annotation.groovy",
        "storage.type.parameters.groovy",
        "storage.type.generic.groovy",
        "storage.type.object.array.groovy",
        "storage.type.primitive.array.groovy",
        "storage.type.primitive.groovy"
      ],
      "settings": { "foreground": "#ffd45f" }
    },
    {
      "name": "Types declaration and references, TS grammar specific",
      "scope": [
        "meta.type.cast.expr",
        "meta.type.new.expr",
        "support.constant.math",
        "support.constant.dom",
        "support.constant.json",
        "entity.other.inherited-class",
        "punctuation.separator.namespace.ruby"
      ],
      "settings": { "foreground": "#ffd45f" }
    },
    {
      "name": "CSS property value",
      "scope": [
        "support.constant.property-value",
        "support.constant.font-name",
        "support.constant.media-type",
        "support.constant.media",
        "constant.other.color.rgb-value",
        "constant.other.rgb-value",
        "support.constant.color"
      ],
      "settings": { "foreground": "#beeaff" }
    },
    {
      "name": "Regular expression groups",
      "scope": [
        "punctuation.definition.group.regexp",
        "punctuation.definition.group.assertion.regexp",
        "punctuation.definition.character-class.regexp",
        "punctuation.character.set.begin.regexp",
        "punctuation.character.set.end.regexp",
        "keyword.operator.negation.regexp",
        "support.other.parenthesis.regexp"
      ],
      "settings": { "foreground": "#beeaff" }
    },
    {
      "scope": [
        "constant.character.character-class.regexp",
        "constant.other.character-class.set.regexp",
        "constant.other.character-class.regexp",
        "constant.character.set.regexp"
      ],
      "settings": { "foreground": "#d16969" }
    },
    {
      "scope": ["keyword.operator.or.regexp", "keyword.control.anchor.regexp"],
      "settings": { "foreground": "#7bc4ff" }
    },
    {
      "scope": "keyword.operator.quantifier.regexp",
      "settings": { "foreground": "#d7ba7d" }
    },
    {
      "scope": ["constant.character", "constant.other.option"],
      "settings": { "foreground": "#ff8484" }
    },
    {
      "scope": "constant.character.escape",
      "settings": { "foreground": "#d7ba7d" }
    },
    { "scope": "entity.name.label", "settings": { "foreground": "#C8C8C8" } }
  ]
}
